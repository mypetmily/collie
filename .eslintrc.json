{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "naver",
    "plugin:react/recommended",
    "prettier",
    "prettier/react",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "import", "eslint-plugin-react-hooks", "@typescript-eslint"],
  "rules": {
    "no-console": "warn",
    "no-use-before-define": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
